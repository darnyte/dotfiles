[user]
	name = Markus Fisch
	email = mf@markusfisch.de

[color]
	ui = auto

[merge]
	renameLimit = 999999

[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f

[pull]
	rebase = false
[init]
	defaultBranch = main
[core]
	eol = lf
